{
  "name": "language-openvpn",
  "version": "0.1.0",
  "description": "Language support and snippets for OpenVPN configuration files",
  "license": "MIT",
  "scripts": {
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "openvpn",
    "open vpn"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-language-openvpn"
  },
  "main": "lib/main",
  "engines": {
